#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mr-nobody/AllProjectsTeach2Give/George-Njunge-QA-Software-Engineering/Assignments/Javascript/13.asynchronuosProgramming/promises/node_modules/.pnpm/touch@3.1.1/node_modules/touch/bin/node_modules:/home/mr-nobody/AllProjectsTeach2Give/George-Njunge-QA-Software-Engineering/Assignments/Javascript/13.asynchronuosProgramming/promises/node_modules/.pnpm/touch@3.1.1/node_modules/touch/node_modules:/home/mr-nobody/AllProjectsTeach2Give/George-Njunge-QA-Software-Engineering/Assignments/Javascript/13.asynchronuosProgramming/promises/node_modules/.pnpm/touch@3.1.1/node_modules:/home/mr-nobody/AllProjectsTeach2Give/George-Njunge-QA-Software-Engineering/Assignments/Javascript/13.asynchronuosProgramming/promises/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mr-nobody/AllProjectsTeach2Give/George-Njunge-QA-Software-Engineering/Assignments/Javascript/13.asynchronuosProgramming/promises/node_modules/.pnpm/touch@3.1.1/node_modules/touch/bin/node_modules:/home/mr-nobody/AllProjectsTeach2Give/George-Njunge-QA-Software-Engineering/Assignments/Javascript/13.asynchronuosProgramming/promises/node_modules/.pnpm/touch@3.1.1/node_modules/touch/node_modules:/home/mr-nobody/AllProjectsTeach2Give/George-Njunge-QA-Software-Engineering/Assignments/Javascript/13.asynchronuosProgramming/promises/node_modules/.pnpm/touch@3.1.1/node_modules:/home/mr-nobody/AllProjectsTeach2Give/George-Njunge-QA-Software-Engineering/Assignments/Javascript/13.asynchronuosProgramming/promises/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../touch/bin/nodetouch.js" "$@"
else
  exec node  "$basedir/../touch/bin/nodetouch.js" "$@"
fi
